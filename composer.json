{
    "name": "casperlaitw/laravel-fb-messenger",
    "type": "library",
    "description": "Laravel facebook messenger chatbot provider",
    "keywords": [
        "facebook",
        "chatbot",
        "messenger"
    ],
    "homepage": "https://github.com/casperlaitw/laravel-fb-messenger",
    "license": "MIT",
    "authors": [
        {
            "name": "Casper Lai",
            "email": "casper.lai@sleepingdesign.com",
            "role": "Developer"
        }
    ],
    "require": {
        "illuminate/support": "~5.1",
        "illuminate/routing": "~5.1",
        "php": ">=5.6.4",
        "pimax/fb-messenger-php": "dev-master",
        "mockery/mockery": "^0.9.5",
        "satooshi/php-coveralls": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.0",
        "nesbot/carbon": "^1.21"
    },
    "autoload": {
        "psr-4": {
            "Casperlaitw\\LaravelFbMessenger\\": "src"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
      "preferred-install": "dist"
    }
}
